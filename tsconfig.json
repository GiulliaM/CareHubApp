{  
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "react",
      "react-native",
      "node"
    ]
  },
  "include": [
    "frontend/src/**/*",
    "App.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
