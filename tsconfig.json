    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"]
    }
    
  },
  "include": [
    "App.tsx",
    "frontend/src/**/*.ts",
    "frontend/src/**/*.tsx",
    "expo-env.d.ts"
